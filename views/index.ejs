<%- include('./partials/header') %>
   


<% if (login) { %>                
    <h1>Usuario conectado: <strong><%= email %></strong></h1>                        
    <a href="logout" class="a-logout">Logout</a><br>
    <a href="preguntas" class="a-logout">preguntas</a>
    <a href="mostrarrespuestas" class="a-logout">Respuestas</a>

<%} else { %>
    <h1><strong><%= name %></strong></h1>   
    <a href="login" class="a-login">Ir a Login</a>
<% } %>



   <% if (typeof alert != "undefined")  { %>
    <script>
    Swal.fire({
      title:'<%= alertTitle %>',
      text:'<%= alertMessage %>',
      icon:'<%= alertIcon %>',
      showConfirmButton: <%= showConfirmButton %>,
      timer: <%= timer %>
    }).then(()=>{
      window.location='/<%= ruta %>'
    })
    </script>
  <% } %>
	</div>

 <%- include('./partials/footer') %>
